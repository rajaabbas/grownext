{
  "name": "ma-boilerplate",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.7",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "test": "turbo run test --filter=!@ma/e2e",
    "e2e:test": "pnpm --filter @ma/e2e test",
    "e2e:test:debug": "pnpm --filter @ma/e2e run test:debug",
    "e2e:test:ui": "pnpm --filter @ma/e2e run test:ui",
    "e2e:codegen": "pnpm --filter @ma/e2e run codegen",
    "db:push": "pnpm --filter @ma/db run db:push",
    "db:migrate": "pnpm --filter @ma/db run db:migrate",
    "db:seed": "pnpm --filter @ma/db run db:seed",
    "worker:enqueue": "pnpm --filter @ma/worker run enqueue"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@types/node": "^20.11.27",
    "@vitest/coverage-v8": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "prettier": "^3.2.5",
    "turbo": "^2.0.3",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
